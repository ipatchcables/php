
<?php

$isMale = true;
$isTall = true;

if ($isMale && isTall;){
  echo "Male";
} elseif ($isMale && !$isTall) {
echo "You are a short male"
}  elseif (!$isMale && $isTall) {
echo "You are not a male"
} else {
echo "End"
}


?>


<?php

function getMax($num1, $num2, $num3){
if ($num1 >= $num2 && $num1 >= $num3){
  reutrn $num1;
  } elseif($num2 >= $num1 && $num2 >= $num3){
  return $num2;
  } else {
  return $num3;
  }
  }
  
